<odoo>
    <record model="ir.ui.view" id="travel_customer_list">
        <field name="name">Travel Customer list</field>
        <field name="model">travel.customer</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="gender"/>
                <field name="destination"/>
                <field name="start_date"/>
                <field name="end_date"/>
            </tree>
        </field>
    </record>
    <record model="ir.ui.view" id="travel_customer_form">
        <field name="name">Travel Customer form</field>
        <field name="model">travel.customer</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <group>
                            <field name="name"/>
                            <field name="gender"/>
                            <field name="destination"/>
                        </group>
                        <group>
                            <field name="start_date"/>
                            <field name="end_date"/>
                            <field name="phone"/>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    <record model="ir.ui.view" id="travel_customer_graph">
    <field name="name">Travel Customer graph</field>
    <field name="model">travel.customer</field>
    <field name="arch" type="xml">
        <graph>
            <field name="gender"/>
            <field name="destination"/>
        </graph>
    </field>
    </record>

    <record model="ir.actions.act_window" id="travel_customer_action_window">
        <field name="name">Travel Customer</field>
        <field name="res_model">travel.customer</field>
        <field name="view_mode">tree,form,graph</field>
    </record>

    <menuitem name="Travels" id="travel_management_menu_root"/>
    <menuitem name="customer" id="travel_management_customer" parent="travel_management_menu_root"
              action="travel_customer_action_window"/>

0
</odoo>