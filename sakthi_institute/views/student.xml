<odoo>
    <menuitem name="Sakthi Institute" id="sakthi_institute_menu_root"/>
    <menuitem name="Student" id="sakthi_institute_student_root" parent="sakthi_institute_menu_root"
              action="sakthi_institute_student_action_window"/>


    <record model="ir.ui.view" id="sakthi_institute_list">
        <field name="name">sakthi institute Student list</field>
        <field name="model">type.student</field>
        <field name="arch" type="xml">
            <tree>
                <field name="sequence" widget='handle'/>
                <field name="register_no"/>
                <field name="name"/>
                <field name="dob"/>
                <field name="age"/>
                <field name="gender"/>
                <field name="section"/>
                <field name="timing"/>
                <field name="fee"/>
                <field name="doj"/>
                <field name="description"/>
            </tree>
        </field>
    </record>

    <record model="ir.actions.act_window" id="sakthi_institute_student_action_window">
        <field name="name">Student</field>
        <field name="res_model">type.student</field>
        <field name="view_mode">tree</field>
    </record>
    <odoo/>


