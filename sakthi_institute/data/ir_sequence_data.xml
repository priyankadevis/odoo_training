<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">
    <record id="seq_sakthi_student" model="ir.sequence">
        <field name="name">Sakthi Student</field>
        <field name="code">sakthi.student</field>
        <field name="prefix">SSTUD/%(year)s/</field>
        <field name="padding">3</field>
        <field name="company_id" eval="False"/>
    </record>
    <record id="seq_sakthi_student_fee" model="ir.sequence">
        <field name="name">Sakthi Student Fee</field>
        <field name="code">student.fee</field>
        <field name="prefix">FEE/%(year)s/</field>
        <field name="padding">6</field>
        <field name="company_id" eval="False"/>
    </record>
</odoo>
